{
  "editor.fontSize": 15,
  "editor.fontFamily": "SF Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.tabSize": 2,
  "atomKeymap.promptV3Features": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.minimap.enabled": false,
  "window.zoomLevel": 1,
  "explorer.confirmDragAndDrop": false,
  "files.autoSave": "onFocusChange",
  "java.import.exclusions": [],
  "java.jdt.ls.vmargs": "-noverify -Xmx1G -XX:+UseG1GC -XX:+UseStringDeduplication -javaagent:\"/Users/ryansobol/.vscode/extensions/gabrielbb.vscode-lombok-0.9.9/server/lombok.jar\"",
  "json.format.enable": false,
  "editor.wordWrapColumn": 88,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "terraform.telemetry.enabled": false,
  "editor.rulers": [
    100
  ],
  "terminal.integrated.fontSize": 16,
  "java.errors.incompleteClasspath.severity": "ignore",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "liveshare.featureSet": "insiders",
  "files.trimTrailingWhitespace": true,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.hidesExplorerArrows": true,
  "material-icon-theme.folders.theme": "classic",
  "explorer.openEditors.visible": 0,
  "diffEditor.renderSideBySide": false,
  "go.formatTool": "goimports",
  "go.useLanguageServer": true,
  "go.buildOnSave": "off",
  "python.jediEnabled": false,
  "workbench.colorTheme": "Tomorrow Night Eighties",
  "editor.renderWhitespace": "boundary",
  "editor.formatOnPaste": true,
  "workbench.editorAssociations": [
    {
      "viewType": "jupyter.notebook.ipynb",
      "filenamePattern": "*.ipynb"
    }
],
"python.languageServer": "Microsoft",
"python.defaultInterpreterPath": "/usr/local/bin/python3",
"files.associations": {
  "*.patch": "diff"
}
}
